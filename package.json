{
  "name": "myretail",
  "version": "1.0.0",
  "description": "rapidly growing company with HQ in Richmond",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "node ./node_modules/webpack/bin/webpack.js",
    "lint": "node ./node_modules/.bin/eslint",
    "test": "jest --coverage --forceExit --watch",
    "test-e2e": "nightwatch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|jpg|png|scss)$": "<rootDir>/empty-module.js"
    },
    "coveragePathIgnorePatterns": [
      "index.dev.js",
      "webpack.config.js",
      "<rootDir>/build/",
      "<rootDir>/mocks",
      "<rootDir>/dev.middleware.js",
      "<rootDir>/test/stylesMock.js"
    ]
  },
  "author": "Rakesh Nallam",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.25.0",
    "env2": "^2.2.0",
    "eslint": "^4.18.1",
    "express": "^4.16.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.7",
    "navalia": "^1.3.0",
    "nightwatch": "^0.9.20",
    "node-sass": "^4.0.2",
    "prop-types": "^15.6.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-native-doc-viewer": "^2.7.3",
    "react-redux": "4.3.0",
    "react-responsive-carousel": "^3.1.33",
    "react-slick": "^0.19.0",
    "react-stars": "^2.2.5",
    "react-test-renderer": "^15.4.1",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "require-hacker": "^3.0.1",
    "sass-loader": "^4.0.2",
    "sinon": "^4.4.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.15.0",
    "webpack-combine-loaders": "^2.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.0",
    "babel-core": "^6.2.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "enzyme-to-json": "^3.0.0-beta6",
    "jest": "^20.0.0",
    "react-addons-test-utils": "^0.14.7",
    "resolve-url-loader": "^2.3.0",
    "selenium-server": "^3.10.0",
    "webpack-cli": "^2.0.9"
  }
}
